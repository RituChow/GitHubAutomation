        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
	if (isLoading || newValue == '') {
		return;
	}
	var ga = new GlideAjax('GitHubAutomationUtil');
	ga.addParam('sysparm_name','getRepo');
	ga.addParam('sysparm_app',newValue);
	ga.getXML(responseCallBack);
}

function responseCallBack(response) {

	var answer = response.responseXML.documentElement.getAttribute('answer');
	
	if(answer != null && answer != undefined)
	{
		var retValue = JSON.parse(answer);
		g_form.setValue('git_repo',retValue.git_repo);
		g_form.setValue('project',retValue.user);
	}
}]]></script>
